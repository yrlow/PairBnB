<% @listing.images.each do |i| %>
	<%= image_tag (i.medium.url) %><br>	
<% end %>

<%= "place name: #{@listing.place_name}" %></br>
<%= "place type: #{@listing.place_type}" %></br>
<%= "address: jalan #{@listing.address.jalan}, #{@listing.address.state}, #{@listing.address.country}" %></br>
<%= "bedroom: #{@listing.bedroom}" %></br>
<%= "bath: #{@listing.bath}" %></br>
<%= "amenity: #{@listing.amenity}" %></br>
<%= "cost per night: #{@listing.cost_per_night}" %></br>
<%= "description: #{@listing.description}" %></br>
<%= link_to "edit", "/listings/#{@listing.id}/edit" %></br>
<%= link_to "delete", "/listings/#{@listing.id}", method: :delete, data: {confirm: 'Are you sure?'} %>
